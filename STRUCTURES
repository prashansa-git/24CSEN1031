// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>
struct student{
    int rollno;
    char name[15];
    float age;
    char gender;
};
int main() {
    struct student s1;
    printf("Size of student structure is %d", sizeof(s1));
    s1.rollno = 2024119926;
    strcpy(s1.name,"prashansa gopi");
    s1.age = 18.9;
    s1.gender = 'F';
    
    printf("\nRoll no: %d",s1.rollno);
    printf("\nName: %s",s1.name);
    printf("\nAge: %f",s1.age);
    printf("\nGender: %c",s1.gender);
    return 0;
}

output

Size of student structure is 28
Roll no: 2024119926
Name: prashansa gopi
Age: 18.900000
Gender: F

=== Code Execution Successful ===

#include <stdio.h>

int main() {
  int var1 = 11;
  int var2 = 6;

  int *ptr1;
  int *ptr2;

  ptr1 = &var1;
  ptr2 = &var2;

  printf("Address of var1: %p\n", ptr1);
  printf("Address of var2: %p\n", ptr2);

  printf("Value of var1: %d\n", *ptr1);
  printf("Value of var2: %d\n", *ptr2);

  return 0;
}  

output
Address of var1: 0x7ffda1da33bc
Address of var2: 0x7ffda1da33b8
Value of var1: 11
Value of var2: 6

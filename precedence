#include <stdio.h>
int main() {
    int a = 7, b = 3, c;
    c = a + b * 3;
    printf("Value of c = %d\n", c);
    
    c = a / b * 4;
    printf("Value of c = %d\n", c);
    
    c = 3 % b * a;
    printf("Value of c = %d\n", c);

    return 0;
}

output
Value of c = 16
Value of c = 8
Value of c = 0
